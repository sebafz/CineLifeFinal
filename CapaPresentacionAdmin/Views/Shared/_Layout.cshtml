<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" type="image/png" href="~/Images/faviconSinFondo.png" />
    <title>@ViewBag.Title - Cinelife (Admin)</title>

    @Styles.Render("~/Content/css")
    <link href="~/Content/extras.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/chart.js@latest/dist/Chart.min.js"></script>
</head>

<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark px-4">
        <!-- Navbar Brand-->
        <img src="~/Images/LogoNavBarOscuro.png" />


        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>

        <!-- Navbar-->
        <ul class="navbar-nav ms-auto me-0 me-md-3 my-2 my-md-0">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="@Url.Action("CerrarSesion","Acceso")">Cerrar sesión</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu navbar-cuerppo-padding">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading navbar-link-padding blanco">Resumen</div>
                        <a class="nav-link" id="menutablero" href="@Url.Action("Index","Home")">
                            <div class="sb-nav-link-icon" id="iconotablero"><i class="fas fa-tachometer-alt"></i></div>
                            Tablero
                        </a>

                        <div id="collapseMantenedor" class="sb-sidenav-menu-heading navbar-link-padding blanco" data-bs-toggle="collapse" href="#collapseExample0" role="button" aria-expanded="false" aria-controls="collapseExample">
                            Mantenimiento <i class="fas fa-caret-down"></i>
                        </div>
                        <div class="collapse" id="collapseExample0"> 
                            <div class="card card-body card-bg-expand">
                                <a class="nav-link" id="menuusuarios" href="@Url.Action("Usuarios","Home")">
                                    <div class="sb-nav-link-icon" id="iconousuarios"><i class="fas fa-users"></i></div>
                                    Usuarios
                                </a>
                                <a class="nav-link" id="menuclientes" href="@Url.Action("Clientes","Home")">
                                    <div class="sb-nav-link-icon" id="iconoclientes"><i class="fas fa-smile"></i></div>
                                    Clientes
                                </a>
                                <a class="nav-link" id="menuproveedores" href="@Url.Action("Proveedores","Home")">
                                    <div class="sb-nav-link-icon" id="iconoproveedores"><i class="fas fa-dolly"></i></div>
                                    Proveedores
                                </a>
                                <a class="nav-link" id="menusedes" href="@Url.Action("Sedes","Home")">
                                    <div class="sb-nav-link-icon" id="iconosedes"><i class="fas fa-map-pin"></i></div>
                                    Sedes
                                </a>
                            </div>
                        </div>
                    
                        <div id="collapseStock" class="sb-sidenav-menu-heading navbar-link-padding blanco" data-bs-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
                            Stock <i class="fas fa-caret-down"></i>
                        </div>
                        <div class="collapse" id="collapseExample">
                            <div class="card card-body card-bg-expand">
                                <a class="nav-link" id="menumovimientos" href="@Url.Action("Movimientos","Mantenedor")">
                                    <div class="sb-nav-link-icon" id="iconomovimientos"><i class="fas fa-truck"></i></div>
                                    Movimientos de stock
                                </a>
                                <a class="nav-link" id="menudepositos" href="@Url.Action("Deposito","Mantenedor")">
                                    <div class="sb-nav-link-icon" id="iconodepositos"><i class="fas fa-store"></i></div>
                                    Depósitos
                                </a>
                                <a class="nav-link" id="menuarticulos" href="@Url.Action("Producto","Mantenedor")">
                                    <div class="sb-nav-link-icon" id="iconoarticulos"><i class="fas fa-boxes"></i></div>
                                    Artículos
                                </a>
                                <a class="nav-link" id="menucategorias" href="@Url.Action("Categoria","Mantenedor")">
                                    <div class="sb-nav-link-icon" id="iconocategorias"><i class="fas fa-table"></i></div>
                                    Categorías
                                </a>
                                <a class="nav-link" id="menumarcas" href="@Url.Action("Marca","Mantenedor")">
                                    <div class="sb-nav-link-icon" id="iconomarcas"><i class="fas fa-bookmark"></i></div>
                                    Marcas
                                </a>
                            </div>
                        </div>

                        <div id="collapseCompras" class="sb-sidenav-menu-heading navbar-link-padding blanco" data-bs-toggle="collapse" href="#collapseExample2" role="button" aria-expanded="false" aria-controls="collapseExample">
                            Compras <i class="fas fa-caret-down"></i>
                        </div>
                        <div class="collapse" id="collapseExample2">
                            <div class="card card-body card-bg-expand">
                                <a class="nav-link" id="menufacturasc" href="@Url.Action("FacturasCompra","Compras")">
                                    <div class="sb-nav-link-icon" id="iconofacturasc"><i class="fas fa-cash-register"></i></div>
                                    Facturas
                                </a>
                                <a class="nav-link" id="menunotasc" href="@Url.Action("NotasCompra","Compras")">
                                    <div class="sb-nav-link-icon" id="icononotasc"><i class="fas fa-money-bill"></i></div>
                                    Notas
                                </a>
                            </div>
                        </div>

                        <div id="collapseVentas" class="sb-sidenav-menu-heading navbar-link-padding blanco" data-bs-toggle="collapse" href="#collapseExample5" role="button" aria-expanded="false" aria-controls="collapseExample">
                            Ventas <i class="fas fa-caret-down"></i>
                        </div>
                        <div class="collapse" id="collapseExample5">
                            <div class="card card-body card-bg-expand">
                                <a class="nav-link" id="menufacturasv" href="@Url.Action("FacturasVenta","Ventas")">
                                    <div class="sb-nav-link-icon" id="iconofacturasv"><i class="fas fa-cash-register"></i></div>
                                    Facturas
                                </a>
                                <a class="nav-link" id="menunotasv" href="@Url.Action("NotasVenta","Ventas")">
                                    <div class="sb-nav-link-icon" id="icononotasv"><i class="fas fa-money-bill"></i></div>
                                    Notas
                                </a>
                            </div>
                        </div>

                        <div id="collapseCine" class="sb-sidenav-menu-heading navbar-link-padding blanco" data-bs-toggle="collapse" href="#collapseExample3" role="button" aria-expanded="false" aria-controls="collapseExample">
                            Cine <i class="fas fa-caret-down"></i>
                        </div>
                        <div class="collapse" id="collapseExample3">
                            <div class="card card-body card-bg-expand">
                                <a class="nav-link" id="menucartelera" href="@Url.Action("Cartelera","Cine")">
                                    <div class="sb-nav-link-icon" id="iconocartelera"><i class="fas fa-sign"></i></div>
                                    Cartelera
                                </a>
                                <a class="nav-link" id="menufunciones" href="@Url.Action("Funciones","Cine")">
                                    <div class="sb-nav-link-icon" id="iconofunciones"><i class="fas fa-video"></i></div>
                                    Funciones
                                </a>
                                <a class="nav-link" id="menupeliculas" href="@Url.Action("Peliculas","Cine")">
                                    <div class="sb-nav-link-icon" id="iconopeliculas"><i class="fas fa-film"></i></div>
                                    Películas
                                </a>
                                <a class="nav-link" id="menusalas" href="@Url.Action("Salas","Cine")">
                                    <div class="sb-nav-link-icon" id="iconosalas"><i class="fas fa-couch"></i></div>
                                    Salas
                                </a>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main class="bg-gris">
                <div class="container-fluid px-4">
                    @RenderBody()
                </div>
            </main>

            <div id="layoutAuthentication_footer">
                <footer class="py-4 bg-gris mt-auto">
                    <div class="container-fluid px-4">
                        <div class="d-flex align-items-center justify-content-between small">
                            <div class="text-muted">Sistemas III - 2022 <i class="fas fa-bolt"></i> Fernandez Zavalía, Díaz, Dagum, Valdez Kao</div>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
    </div>




    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

<script>
    jQuery(function ($) {
        $.datepicker.regional.sv = {
            closeText: 'Cerrar',
            prevText: 'Anterior',
            nextText: 'Siguiente;',
            currentText: 'Hoy',
            monthNames: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
            monthNamesShort: ['En', 'Feb', 'Mar', 'Abr', 'May', 'Jun', 'Jul', 'Ago', 'Sep', 'Oct', 'Nov', 'Dic'],
            dayNamesShort: ['Lun', 'Mar', 'Mier', 'Jue', 'Vie', 'Sab', 'Dom'],
            dayNames: ['Lunes', 'Martes', 'Miércoles', 'Jueves', 'viernes', 'Sábado', 'Domingo'],
            dayNamesMin: ['Lun', 'Mar', 'Mier', 'Jue', 'Vie', 'Sab', 'Dom'],
            weekHeader: 'Lu',
            dateFormat: 'dd/mm/yy',
            firstDay: 0,
            isRTL: false,
            showMonthAfterYear: false,
            yearSuffix: ''
        };

        $.datepicker.setDefaults($.datepicker.regional.sv);
    });
</script>
